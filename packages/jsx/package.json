  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ueberdosis/tiptap",
    "directory": "packages/jsx"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c"
