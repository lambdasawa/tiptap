{
  "name": "@tiptap/jsx",
  "description": "statically render Tiptap JSON",
  "version": "3.0.0-next.3",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap static renderer",
    "tiptap react renderer"
  ],
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
